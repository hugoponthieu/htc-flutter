{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "htc_flutter_mock",
            "request": "launch",
            "type": "dart",
            "args": [
                "-d",
                "chrome",
                "--web-port=4200",
                "--dart-define",
                "APP_ENV=mock",
            ],
            "program": "lib/main.dart"
        },
        {
            "name": "htc_flutter_dev",
            "request": "launch",
            "type": "dart",
            "args": [
                "-d",
                "chrome",
                "--web-port=4200",
                "--dart-define",
                "APP_ENV=dev",
            ],
            "program": "lib/main.dart"
        },
        {
            "name": "htc_flutter (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "htc_flutter (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}